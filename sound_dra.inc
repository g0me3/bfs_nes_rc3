SSSLO	=	$FE	
SSSHI	=	$FF	

ENABWORD	=	$0400
VOICTEST 	=	ENABWORD+$01
STPINDEX 	=	VOICTEST+$04
SEQINDEX 	=	STPINDEX+$04
SEQPOSIT 	=	SEQINDEX+$04
STEPNUMB 	=	SEQPOSIT+$04
SPDCOUNT 	=	STEPNUMB+$04
SPDINDEX 	=	SPDCOUNT+$04
REALPLAY	=	SPDINDEX+$04
NOTEINIT	=	REALPLAY+$04
REALSOUN 	=	NOTEINIT+$04
LENGCOPY 	=	REALSOUN+$04
NOTELENG 	=	LENGCOPY+$04
REALNOTE 	=	NOTELENG+$04
LOFRQWRD	=	REALNOTE+$04
HIFRQWRD	=	LOFRQWRD+$04
TRANBYTE 	=	HIFRQWRD+$04
SFXTRANS	=	TRANBYTE+$04 
ARPINDEX	=	SFXTRANS+$04
ARPNUMBO	=	ARPINDEX+$04
ARPONOTO	=	ARPNUMBO+$04
VIBGLBYT	=	ARPONOTO+$04
GLDSPEED	=	VIBGLBYT+$04
GLDDIREC	=	GLDSPEED+$04
GLDDELAY	=	GLDDIREC+$04
DESTNOTE	=	GLDDELAY+$04
VIBDELAY	=	DESTNOTE+$04
VIBINDEX	=	VIBDELAY+$04
CONTINOS	=	VIBINDEX+$04
AMPLITUD 	=	CONTINOS+$04
AMPINDEX	=	AMPLITUD+$04
FX01	=	AMPINDEX+$04
FX02	=	FX01+$04	
FX03	=	FX02+$04	
PULINDEX	=	FX03+$04	
PULSBYT1 	=	PULINDEX+$02

	JMP	AUDIOCL
	JMP	AUDIOIN
	JMP	AUDIOPL

AUDIOCL	LDA	#$00
	STA	ENABWORD
	RTS

AUDIOIN	ASL	A
	STA	SEQPOSIT,X	

	LDA	ENABWORD
	ORA	ENABCHAN,X
	STA	ENABWORD
	STA	$4015
	LDA	#$00	 
	STA	SEQINDEX,X
 	STA	SPDINDEX,X
	STA	TRANBYTE,X
	STA	SFXTRANS,X
	STA	STEPNUMB,X
	STA	AMPINDEX,X
	STA	PULINDEX,X
	STA	AMPLITUD,X
	STA	TRANBYTE,X
	STA	CONTINOS,X
     	STA	SPDCOUNT,X
             	LDA	#$01
	STA	VOICTEST,X
	STA	LENGCOPY,X
	STA	STPINDEX,X
	RTS

ENABCHAN	DB	$01,$02,$04,$08

AUDIOPL	LDA	VOICTEST,X
	BEQ	AUDIOEX
             	LDA	ENABWORD
             	STA	$4015
	CPX	#$02
	BCC	VOICE0102
       	BEQ	VOICE03
       	BCS	VOICE04
AUDIOEX	RTS


VOICE04	TXA
	ASL	A
	ASL	A
	TAY
	LDA	#$10		;SETAMPLITUDE             <BIT 04>
	ORA	#$20  		;SETCONTINUES             <BIT 05>
	ORA	AMPLITUD,X	;AMPLITUDE/ENVELOPE-VALUE <BIT 03-00>
	STA	$4000,Y
	LDA	LOFRQWRD,X
	STA	$4002,Y
	LDA	#$F8
	STA	$4003,Y
	JMP	JMPVO04	

VOICE03	TXA
	ASL	A
	ASL	A
	TAY
	LDA	#$80
	JMP	JMPVO03
VOICE0102

SETVOI1	TXA
	ASL	A
	ASL	A
	TAY
	LDA	PULSBYT1,X    	;SETPULSE                 <BIT 07-06>
	ORA	#$10    		;SETAMPLITUDE             <BIT 04>
	ORA	#$20		;SETCONTINUES             <BIT 05>
JMPVO03	ORA	AMPLITUD,X	;AMPLITUDE/ENVELOPE-VALUE <BIT 03-00>
	STA	$4000,Y

	LDA	#$08		;SET-HARDWARE-GLIDE-DOWN		<BIT 03>
	STA	$4001,Y

	LDA	LOFRQWRD,X	;FREQUENCY-SET-ROUTINE
	STA	$4002,Y

	LDA	HIFRQWRD,X
	CMP	CONTINOS,X
	BEQ	NOHINO1
	STA	$4003,Y	
	STA	CONTINOS,X
NOHINO1

JMPVO04

SPDHDL1	LDA	NOTELENG,X	;CUT-OFF-AT-END-ROUTINE
	CMP	#$01
	BEQ	SPEEDROUT
	LDA	LENGCOPY,X
	CMP	#$01
	BNE	SPEEDROUT
	LDA	SPDCOUNT,X
	BNE	SPEEDROUT
	LDA	#$00
	STA	AMPLITUD,X
SPEEDROUT
	
	
	LDA	#$00		;SPEED-ROUTINE
	STA	REALPLAY,X
	DEC	SPDCOUNT,X
	BPL	SPDHDX1

	LDY	SEQPOSIT,X
	LDA	SPDTABEL+$00,Y
	STA	SSSLO
	LDA	SPDTABEL+$01,Y
	STA	SSSHI

	LDY	SPDINDEX,X
	LDA	(SSSLO),Y
	CMP	#$FF
	BNE	SETSPEED
	LDY	#$00
	LDA	(SSSLO),Y
SETSPEED	STA	SPDCOUNT,X
	INY
	TYA
	STA	SPDINDEX,X
	LDA	#$FF
	STA	REALPLAY,X
SPDHDX1	
       	LDA	REALPLAY,X	;CHECK IF NEW LENG SUBTRACTION NECESSARY
	BPL	NOSTPH1	

AUDIPL1	DEC	LENGCOPY,X	;DECREASE SPEED, IF ZERO THEN STEPHANDLE
	BEQ	STPHDL1	

NOSTPH1	LDA	REALNOTE,X	;CHECK REALNOTE ON PAUSE, NO FX
	CMP	#$FF
	BEQ	NOFXPL1
	JSR	PLAYFIX

NOFXPL1	RTS

STPHDL1	LDY	STEPNUMB,X	;WHAT STEP TO TAKE
	LDA	STPTABLO,Y
	STA	SSSLO
	LDA	STPTABHI,Y
	STA	SSSHI

	LDA	#$00		;RESET GLIDE (IF NOT REACHED DEST. NOTE)
	STA	GLDSPEED,X

	LDY	STPINDEX,X	;READ STEP-INDEX
	LDA	(SSSLO),Y
	CMP	#$FF		;STEP CHECK ON END 
	BEQ	SEQHDL1		;YES, GOTO SEQUENCE-HANDLE
	JMP	STPHD12		;NO, GOTO STEPHANDLE 02

SEQHDL1	
	LDY	SEQPOSIT,X	;WHAT SEQUENCE TO TAKE
	LDA	SEQTABEL+$00,Y	;
	STA	SSSLO		;
	LDA	SEQTABEL+$01,Y	;
	STA	SSSHI		;

	LDY	SEQINDEX,X	;READ SEQUENCE-INDEX
	LDA	(SSSLO),Y		;
	CMP	#$FF		;CHECK SEQUENCE ON END-REPEAT
	BNE	SEQHD12		;
	INY			;
	LDA	(SSSLO),Y		;WHERE TO JUMP BACK IN SEQUENCE
	STA	SEQINDEX,X	;
	TAY			;
	LDA	(SSSLO),Y		;
	JMP	SEQHD15		;GOTO NEXT SEQUENCE

SEQHD12	CMP	#$FE		;CHECK SEQUENCE ON END-REAL
	BNE	SEQHD13		;
	LDA	#$00		;YES, DISABLE VOICE
	STA	VOICTEST,X	;
	STA	AMPLITUD,X
	RTS

SEQHD13	
SEQHD15	CMP	#$80		;NOTE TRANSPOSE	CHECK
	BCC	SEQHD16  	      
       	AND	#$7F
NOTRNSI	STA	TRANBYTE,X
	INY
	LDA	(SSSLO),Y
	BMI	NONWSTP

SEQHD16	CMP	#$70
	BCC	SEQHD17
	AND	#$0F
	STA	SFXTRANS,X
	INY
	LDA	(SSSLO),Y

SEQHD17	STA	STEPNUMB,X	;SET STEP & CLEAR STEPINDEX
	INY
NONWSTP	TYA
	STA	SEQINDEX,X
	LDA	#$00
	STA	STPINDEX,X
	JMP	STPHDL1		;BACK TO STEP-HANDLE

STPHD12	CMP	#$FD		;GLIDE CHECK IN STEP
	BNE	STPHD14
	INY
	LDA	(SSSLO),Y
	STA	GLDSPEED,X
	INY
	LDA	(SSSLO),Y
	CLC
	ADC	TRANBYTE,X
	STA	REALNOTE,X
	INY
	LDA	(SSSLO),Y
	CLC
	ADC	TRANBYTE,X
	STA	DESTNOTE,X
	INY
	JMP	NONEWN1

STPHD14	CMP	#$E0		;PAUSE CHECK
	BCC	STPHD15
	AND	#$1F
	STA	NOTELENG,X
	LDA	#$FF
	STA	REALNOTE,X
	LDA	#$00
	STA	AMPLITUD,X
	INY
	TYA
	STA	STPINDEX,X
	JMP	PAUSEX1

STPHD15	CMP	#$C0		;SOUND CHECK
	BCC	STPHD16
	AND	#$1F
	CLC
	ADC	SFXTRANS,X
	ASL	A
	ASL	A
	STA	REALSOUN,X
;	LDA	SEQPOSIT,X
;	CMP	#$30*2
;	BCC	NOEXTRSN
;	LDA	REALSOUN,X
;	CLC
;	ADC	#$80
;	STA	REALSOUN,X

NOEXTRSN	INY
	LDA	(SSSLO),Y
	CMP	#$FD	
	BEQ	STPHD12
	CMP	#$C0
	BCS	NONEWN1
	
STPHD16	CMP	#$80		;LENGTH CHECK
	BCC	STPHD17
	AND	#$7F
	STA	NOTELENG,X
	INY
	LDA	(SSSLO),Y
	CMP	#$FD
	BEQ	STPHD12
	CMP	#$80
	BCS	NONEWN1

STPHD17	CMP	#$60		;ARP CHECK
	BCC	STPHD18
	AND	#$1F	
	STA	ARPNUMBO,X
;	LDA	SEQPOSIT,X
;	CMP	#$30*2
;	BCC	NOEXTARP
;	LDA	ARPNUMBO,X
;	CLC
;	ADC	#$20
;	STA	ARPNUMBO,X
NOEXTARP	INY
	LDA	(SSSLO),Y
	CMP	#$60
	BCS	NONEWN1

STPHD18	CLC			;SET NOTE + NOTE TRANSPOSE
	ADC	TRANBYTE,X	;
	STA	REALNOTE,X	;
	INY			;
NONEWN1	TYA
	STA	STPINDEX,X	;RESTORE STEP-INDEX

	LDY	REALSOUN,X	;
	CPX	#$02		;CHECK SOUND-INIT VOICE 01
	BCS	INITV0304		;

INITV12	LDA	SOUNDFIX+$00,Y	;CHECK IF NEW PULSE TO BE SET
	AND	#$08		;
	BNE	NONEWPL		;
	LDA	SOUNDFIX+$00,Y	;YES, SET NEW PULSE
	AND	#$C0		;
	STA	PULSBYT1,X     	;
	LDA	#$00
	STA	PULINDEX,X
INITV0304

NONEWPL	LDA	SOUNDFIX+$01,Y	;SET VIBRATO-BYTE INTO FX01 & SET EFFECTS (ARP, ETC)
	STA	FX01,X		;
	LDA	SOUNDFIX+$02,Y	;SET VIBDELAY PULTAB-NUMBER INTO FX02
	STA	FX02,X		;
	
NONEWG1	LDA	SOUNDFIX+$03,Y	;SET EFFECT-VALUES INTO FX03	
	STA	FX03,X		;
	CPX	#$03
	BEQ	NFORV04

	LDY	REALNOTE,X	;READ REALNOTE AS INDEX
	LDA	FRQTABLO,Y 	;LOW-FREQUENCY READ <BIT 07-00>
	STA	LOFRQWRD,X	;

	LDA	#$F8		;HARDWARE LENGTH ENDLESS <BIT 07-03>
	ORA	FRQTABHI,Y 	;HI-FREQUENCY READ <BIT 02-00>
	STA	HIFRQWRD,X	;HF SET
	JMP	PAUSEX1

NFORV04	LDA	REALNOTE,X
	STA	LOFRQWRD,X

PAUSEX1	LDA	NOTELENG,X
       	STA	LENGCOPY,X

	LDA	#$00
      	STA	AMPINDEX,X
	STA	VIBINDEX,X
       	STA	ARPINDEX,X
;----------------------------------------------------------------------------
INTPLA1	LDA	GLDSPEED,X
	BEQ	FXPLA12
	TAY	
	LSR	A
	LSR	A
	LSR	A
	LSR	A
	STA	GLDDELAY,X
	TYA
	AND	#$0F
	STA	GLDSPEED,X

	LDA	NOTELENG,X
	SEC
	SBC	GLDDELAY,X
	STA	GLDDELAY,X
	LDA	REALNOTE,X
	SEC
	SBC	DESTNOTE,X
	STA	GLDDIREC,X

FXPLA12	LDA	FX02,X
	LSR	A
	LSR	A
	LSR	A
	LSR	A
	STA	VIBDELAY,X
	LDA	NOTELENG,X
	SEC
	SBC	VIBDELAY,X
	BCS	REFIX01
	LDA	FX01,X
	AND	#$C0
	STA	FX01,X
	JMP	EXITIN1
REFIX01	STA	VIBDELAY,X
EXITIN1      	RTS

PLAYFIX
             ;LDA	LENGCOPY,X	;CHECK IF THE REAL NOTELENGTH IS 1
             ;CMP	#$01
             ;BNE	NOAMPCUT		;IF SO, DO NOT CUT NOTE OFF AT END

             ;LDA	NOTELENG,X
             ;CMP	#01
             ;BNE	NOAMPCUT
             ;LDA	SPDCOUNT,X	;!
             ;CMP	#01		;!
             ;BCS	NOAMPCUT		;!
             ;LDA	#$01
             ;STA	AMPLITUD,X
             ;JMP	NOAMPL1	
NOAMPCUT



	LDA	FX03,X		   ;AMPLITUDE-ROUTINE		
       	AND	#$7F			
       	TAY
       	LDA	AMPTABL,Y
       	STA	SSSLO
       	LDA	AMPTABH,Y
       	STA	SSSHI
       
       	LDY	AMPINDEX,X
       	LDA	(SSSLO),Y
 	CMP	#$FE
       	BEQ	NOAMPL1
       	STA	AMPLITUD,X
       	INC	AMPINDEX,X
NOAMPL1	

GLIDE1	LDY	GLDSPEED,X	  ;PORTAMENTO-ROUTINE
	BEQ	NOGLD12

	LDA	GLDDELAY,X
	CMP	LENGCOPY,X
	BCS	DOPORTA
	JMP	PORTABIZZY
DOPORTA
	LDY	GLDDIREC,X
	BMI	DOWNGL1
	LDA	LOFRQWRD,X
	CLC
	ADC	GLDSPEED,X
	STA	LOFRQWRD,X
	BCC	CHECKV1
       	INC	HIFRQWRD,X

CHECKV1	LDY	DESTNOTE,X
	SEC
	SBC	FRQTABLO,Y
	BCC	NOGLD01
	BCS	FROTER1

DOWNGL1	LDA	LOFRQWRD,X
	SEC
	SBC	GLDSPEED,X
	STA	LOFRQWRD,X
	BCS	CHECKV2
	DEC	HIFRQWRD,X
	
CHECKV2	LDY	DESTNOTE,X
	SEC
	SBC	FRQTABLO,Y
	BCS	NOGLD01
FROTER1	TYA
	STA	REALNOTE,X
       	LDA	FRQTABLO,Y
       	STA	LOFRQWRD,X
       	LDA	FRQTABHI,Y
       	STA	HIFRQWRD,X
	
	LDA	#$00
	STA	GLDSPEED,X
NOGLD01	JMP	NOVIB01
NOGLD12

	LDA	GLDSPEED,X
	BNE	PORTABIZZY

VIBRA01	LDA	FX01,X	 	;VIBRATO-ROUTINE
             	AND	#$0F
             	BEQ	NOVIB01
             	LDA	VIBDELAY,X
             	CMP	LENGCOPY,X
             	BCC	NOVIB01

             	LDA	FX01,X
             	AND	#$0F
             	TAY
             	LDA	VIBTABL-1,Y
             	STA	SSSLO
             	LDA	VIBTABH-1,Y
             	STA	SSSHI

             	LDY	VIBINDEX,X
             	LDA	(SSSLO),Y
             	CMP	#$80
	BNE	VIB01
	LDY	#$00
VIB01	LDA	(SSSLO),Y
	BPL	UPVIBOS
	EOR	#$FF
	CLC
	ADC	#$01
	STA	SSSLO
	LDA	LOFRQWRD,X
	SEC
	SBC	SSSLO
	STA	LOFRQWRD,X
	BCS	NOHIGH1
	DEC	HIFRQWRD,X
NOHIGH1	JMP	VIBNO01
       
UPVIBOS	LDA	LOFRQWRD,X
	CLC
	ADC	(SSSLO),Y
	STA	LOFRQWRD,X
	BCC	NOHIGH2
       	INC	HIFRQWRD,X
NOHIGH2		
VIBNO01	INY
	TYA
	STA	VIBINDEX,X
NOVIB01

PORTABIZZY

PULSE1 	CPX	#$02
       	BCS	NOPULS1
       	LDA	FX02,X	 	;PULSE ROUTINE
       	AND	#$0F
       	BEQ	NOPULS1
       	TAY	      
